{
  "id": "genefunk2090",
  "title": "GeneFunk2090",
  "description": "A system for playing the GeneFunk2090 in the Foundry Virtual Tabletop environment.",
  "version": "2.4.0",
  "compatibility": {
    "minimum": "10.303",
    "verified": "11"
  },
  "url": "https://github.com/foundryvtt/genefunk2090/",
  "manifest": "https://raw.githubusercontent.com/ivan-brock/genefunk2090/HelloWorld/system.json",
  "download": "https://github.com/ivan-brock/genefunk2090/archive/refs/tags/HelloWorld.zip",
  "authors": [
    {
      "name": "Doof",
      "url": "https://foundryvtt.com",
      "flags": {}
    }
  ],
  "esmodules": [
    "genefunk2090.mjs"
  ],
  "styles": [
    "genefunk2090.css"
  ],
  "packs": [
    {
      "name": "heroes",
      "label": "Starter Heroes",
      "system": "genefunk2090",
      "path": "packs/heroes.db",
      "type": "Actor",
      "private": false,
      "flags": {}
    },
    {
      "name": "monsters",
      "label": "Monsters (SRD)",
      "system": "genefunk2090",
      "path": "packs/monsters.db",
      "type": "Actor",
      "private": false,
      "flags": {}
    },
    {
      "name": "items",
      "label": "Items (SRD)",
      "system": "genefunk2090",
      "path": "packs/items.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "tradegoods",
      "label": "Trade Goods (SRD)",
      "system": "genefunk2090",
      "path": "packs/tradegoods.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "spells",
      "label": "Spells (SRD)",
      "system": "genefunk2090",
      "path": "packs/spells.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "backgrounds",
      "label": "Backgrounds (SRD)",
      "system": "genefunk2090",
      "path": "packs/backgrounds.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "classes",
      "label": "Classes (SRD)",
      "system": "genefunk2090",
      "path": "packs/classes.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "subclasses",
      "label": "Subclasses (SRD)",
      "system": "genefunk2090",
      "path": "packs/subclasses.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "classfeatures",
      "label": "Class Features (SRD)",
      "system": "genefunk2090",
      "path": "packs/classfeatures.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "races",
      "label": "Races (SRD)",
      "system": "genefunk2090",
      "path": "packs/races.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "monsterfeatures",
      "label": "Monster Features (SRD)",
      "system": "genefunk2090",
      "path": "packs/monsterfeatures.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "rules",
      "label": "Rules (SRD)",
      "system": "genefunk2090",
      "path": "packs/rules.db",
      "type": "JournalEntry",
      "private": false,
      "flags": {}
    },
    {
      "name": "tables",
      "label": "Tables (SRD)",
      "system": "genefunk2090",
      "path": "packs/tables.db",
      "type": "RollTable",
      "private": false,
      "flags": {}
    }
  ],
  "packFolders": [
    {
      "name": "GeneFunk2090 SRD Content",
      "sorting": "m",
      "color": "#cd2c1e",
      "packs": [
        "rules",
        "tables",
        "heroes"
      ],
      "folders": [
        {
          "name": "Character Features",
          "sorting": "m",
          "color": "#b72c1f",
          "packs": [
            "classes",
            "subclasses",
            "classfeatures",
            "backgrounds",
            "races"
          ]
        },
        {
          "name": "Items & Spells",
          "sorting": "m",
          "color": "#941919 ",
          "packs": [
            "items",
            "spells",
            "tradegoods"
          ]
        },
        {
          "name": "Monsters",
          "sorting": "m",
          "color": "#7d160c",
          "packs": [
            "monsters",
            "monsterfeatures"
          ]
        }
      ]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    }
  ],
  "media": [
    {
      "type": "cover",
      "loop": false,
      "flags": {}
    }
  ],
  "socket": true,
  "gridDistance": 5,
  "gridUnits": "ft",
  "primaryTokenAttribute": "attributes.hp",
  "flags": {
    "needsMigrationVersion": "2.4.0",
    "compatibleMigrationVersion": "0.8",
    "hotReload": {
      "extensions": ["css", "hbs", "json"],
      "paths": ["genefunk2090.css", "templates", "lang"]
    }
  }
}
